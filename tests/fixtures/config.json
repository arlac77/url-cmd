{
  "schemes": {
    "assembla": {
      "base": "svn+https",
      "options": {
        "proxy": "http://localhost:3128",
        "credentials": {
          "user": "{ENVIRONMENT}SVN_USER",
          "password": "{ENVIRONMENT}SVN_PASSWORD"
        }
      },
      "prefix": "https://assembla.com"
    },
    "tmp": {
      "base": "file",
      "prefix": "file:///tmp"
    }
  }
}
