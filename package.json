{
  "name": "url-cmd",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "docs": "documentation readme src/url-cmd.js --section=API",
    "semantic-release": "semantic-release",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c && chmod +x bin/url-cmd",
    "lint": "documentation lint src/url-cmd.js"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "execa": "^0.8.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.1",
    "rollup": "^0.52.3",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "semantic-release": "^11.0.2",
    "xo": "^0.19.0",
    "documentation": "^5.3.5"
  },
  "engines": {
    "node": ">=8.9.3"
  },
  "version": "0.0.0-semantic-release",
  "description": "cli tool to work with url resources",
  "main": "src/url-cmd.js",
  "module": "src/url-cmd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "caporal": "^0.9.0",
    "config-expander": "9.0.1",
    "fs-resolver-fs": "3.0.2",
    "ora": "^1.3.0",
    "sftp-resolver-fs": "^1.2.0",
    "svn-dav-fs": "^5.1.2",
    "url-resolver-fs": "5.2.3"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "keywords": [
    "url",
    "file system"
  ]
}
