{
  "name": "arlac77/url-util",
  "scripts": {
    "cover": "nyc ava",
    "precover": "rollup -c rollup.config.test.js",
    "test": "ava",
    "pretest": "rollup -c rollup.config.test.js",
    "posttest": "markdown-doctest",
    "prepublish": "rollup --output=dist/index.js -c -- {{module}}",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "jsdoc-babel": "^0.3.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.3.2",
    "ava": "^0.19.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "semantic-release": "^6.3.6",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "version": "0.0.0-semantic-release",
  "description": "{{description}}",
  "main": "dist/index.js",
  "keywords": [
    "npm-package-template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/url-util.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-util/issues"
  },
  "homepage": "https://github.com/arlac77/url-util#readme",
  "dependencies": {},
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "tests/build/bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "tests/build/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}