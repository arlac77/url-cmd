{
  "name": "url-cmd",
  "version": "0.0.0-semantic-release",
  "main": "src/url-cmd.js",
  "module": "src/url-cmd.js",
  "description": "cli tool to work with url resources",
  "keywords": [
    "url",
    "file system"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "docs": "documentation readme src/url-cmd.js --section=API",
    "semantic-release": "semantic-release",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c && chmod +x bin/url-cmd",
    "lint": "documentation lint src/url-cmd.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "caporal": "^0.10.0",
    "config-expander": "9.1.2",
    "fs-resolver-fs": "3.0.2",
    "ora": "^2.0.0",
    "sftp-resolver-fs": "^1.2.0",
    "svn-dav-fs": "^5.2.1",
    "url-resolver-fs": "5.2.8"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "documentation": "^6.3.0",
    "execa": "^0.10.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.7.1",
    "rollup": "^0.58.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "semantic-release": "^15.1.7",
    "travis-deploy-once": "^4.4.1"
  },
  "engines": {
    "node": ">=8.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
