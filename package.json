{
  "name": "url-cmd",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/url-cmd.js",
  "module": "src/url-cmd.js",
  "description": "cli tool to work with url resources",
  "keywords": [
    "url",
    "file system"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/url-cmd.js --section=API",
    "lint": "documentation lint src/url-cmd.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "caporal": "^1.1.0",
    "config-expander": "^10.0.1",
    "fs-resolver-fs": "^6.0.14",
    "sftp-resolver-fs": "^2.0.3",
    "svn-dav-fs": "^6.0.16",
    "svn-simple-auth-provider": "^1.0.5",
    "url-resolver-fs": "^7.3.6"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.1.0",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.4.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "ava": {
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
