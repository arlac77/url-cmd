{
  "name": "url-cmd",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/url-cmd-cli.mjs",
  "description": "cli tool to work with url resources",
  "keywords": [
    "url",
    "file system"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/url-cmd-cli.mjs --section=API",
    "lint": "documentation lint src/url-cmd-cli.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "npm run prepare",
    "test": "ava"
  },
  "dependencies": {
    "commander": "^4.1.0",
    "config-expander": "^11.0.9",
    "fs-resolver-fs": "^7.0.1",
    "sftp-resolver-fs": "^2.0.3",
    "svn-dav-fs": "^7.0.0",
    "svn-simple-auth-provider": "^1.1.2",
    "url-resolver-fs": "^8.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "execa": "^4.0.0",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.30.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.2",
    "semantic-release": "^16.0.4"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
