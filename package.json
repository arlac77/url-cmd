{
  "name": "url-cmd",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "module": "src/url-cmd-cli.mjs",
  "description": "cli tool to work with url resources",
  "keywords": [
    "url",
    "file system"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "npm run prepare",
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/url-cmd-cli.mjs --section=API",
    "lint": "documentation lint src/url-cmd-cli.mjs"
  },
  "dependencies": {
    "commander": "^4.1.1",
    "config-expander": "^11.0.11",
    "fs-resolver-fs": "^7.0.1",
    "sftp-resolver-fs": "^2.0.3",
    "svn-dav-fs": "^7.0.0",
    "svn-simple-auth-provider": "^1.1.3",
    "url-resolver-fs": "^8.0.6"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^3.5.2",
    "builtin-modules": "^3.1.0",
    "c8": "^7.1.0",
    "documentation": "^12.1.4",
    "execa": "^4.0.0",
    "rollup": "^2.3.1",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.2",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-cli-app",
      "arlac77/template-github"
    ]
  }
}
