{
  "name": "url-cmd",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/url-cmd.js",
  "module": "src/url-cmd.js",
  "description": "cli tool to work with url resources",
  "keywords": [
    "url",
    "file system"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "docs": "documentation readme src/url-cmd.js --section=API",
    "semantic-release": "semantic-release",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint src/url-cmd.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "caporal": "^0.10.0",
    "config-expander": "^9.1.8",
    "fs-resolver-fs": "^6.0.12",
    "sftp-resolver-fs": "^2.0.3",
    "svn-dav-fs": "^6.0.11",
    "svn-simple-auth-provider": "^1.0.1",
    "url-resolver-fs": "^7.2.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "documentation": "^8.1.2",
    "execa": "^1.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^13.0.1",
    "rollup": "^0.65.1",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.9.12",
    "travis-deploy-once": "^5.0.7",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-cleanup": "^3.0.0"
  },
  "engines": {
    "node": ">=10.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
