{
  "name": "url-cmd",
  "bin": {
    "url-cmd": "bin/url-cmd"
  },
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c && chmod +x bin/url-cmd"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "execa": "^0.8.0",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.2",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.3.0",
    "rollup": "^0.51.6",
    "rollup-plugin-babel": "^3.0.2",
    "semantic-release": "^9.1.1",
    "xo": "^0.19.0",
    "babel-preset-env": "^1.6.1"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "version": "0.0.0-semantic-release",
  "description": "cli tool to work with url resources",
  "main": "src/url-cmd.js",
  "module": "src/url-cmd.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/url-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-cmd/issues"
  },
  "homepage": "https://github.com/arlac77/url-cmd#readme",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "caporal": "^0.8.0",
    "config-expander": "9.0.1",
    "fs-resolver-fs": "3.0.2",
    "ora": "^1.3.0",
    "sftp-resolver-fs": "^1.2.0",
    "svn-dav-fs": "5.1.0",
    "url-resolver-fs": "5.2.3"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "xo": {
    "space": true
  },
  "ava": {
    "files": [
      "tests/*test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "keywords": [
    "url",
    "file system"
  ]
}
